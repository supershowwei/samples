          <Default>table_name</Default>
        </Literal>
      </Declarations>
      <Code Language="SQL Server" Kind="SQL Server"><![CDATA[SELECT *
FROM $table_name$ WITH (NOLOCK)
WHERE$CARET$]]></Code>
